{
	"name": "fprochazka/travis-local-build",
	"require": {
		"php": ">=7.1",
		"nette/utils": "^2.4",
		"symfony/console": "^3.2",
		"symfony/filesystem": "^3.2",
		"symfony/finder": "^3.2",
		"symfony/process": "^3.2",
		"symfony/yaml": "^3.2",
		"tracy/tracy": "^2.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.1",
		"phpstan/phpstan-shim": "^0.11.4"
	},
	"autoload": {
		"psr-4": {
			"Fprochazka\\TravisLocalBuild\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fprochazka\\TravisLocalBuild\\": "tests/"
		}
	},
	"bin": [
		"bin/travis-local"
	]
}
